<launch>
<node pkg="joy" type="joy_node" name="joy1">
    <remap from="joy" to="joy_rk"/>
    <param name="dev" value="/dev/input/js0" /> <!-- Customize this to match the location your joystick is plugged in on-->
    <param name="deadzone" value="0.2" />
    <param name="autorepeat_rate" value="10" />
    <param name="coalesce_interval" value="0.02" />
  </node>
</launch>

